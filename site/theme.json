{
  "colors": {
    "brandPrimary": "#feb8c8",
    "brandSecondary": "white"
  },
  "buttons": {
    "primary": {
      "normal": {
        "default": {
          "text": {
            "color": "$colors.brandSecondary"
          },
          "background": {
            "background-color": "$colors.brandPrimaryDark05"
          }
        },
        "hover": {
          "text": {
            "color": "$colors.brandPrimary"
          },
          "background": {
            "background-color": "$colors.brandSecondary"
          }
        },
        "press": {
          "background": {
            "background-color": "$colors.brandPrimaryDark10"
          },
          "text": {
            "color": "$colors.brandSecondary"
          }
        },
        "focus": {
          "background": {
            "outline-color": "$colors.brandPrimaryClear"
          }
        }
      }
    }
  }
}